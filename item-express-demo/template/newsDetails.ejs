    <div class="">
        <!-- 公共top start -->
        <div class="common-top">
            <div class="logo fl">
                <img src="img/white-log.png" />
            </div>
            <div class="nav fr">
                <img class="nav-img" src="img/nav-icon.png" />
                <div class="nav-list">
                    <div class="title">APPBAR</div>
                    <div class="item">
                        <p>
                            <a href="index.html">首页 </a>
                        </p>
                    </div>
                    <div class="item">
                        <p>
                           品牌历史
                                <img class="right" src="img/right-icon.png" />
                        </p>
                        <ul>
                            <li><a href="brandHistory.html">品牌历史</a></li>
                            <li><a href="brandHeritage.html">品牌传承</a></li>
                            <li><a href="brandConcept.html">品牌理念</a></li>
                            <li><a href="">产地视角</a></li>
                        </ul>
                    </div>
                    <div class="item">
                        <p>
                            <a href="">产品展示
                                <img class="right" src="img/right-icon.png" />
                            </a>
                        </p>
                        <ul>
                            <li><a href="proList.html">产品展示</a></li>
                        </ul>
                    </div>
                    <div class="item">
                        <p>
                            <a href="">婴孕健康
                                <img class="right" src="img/right-icon.png" />
                            </a>
                        </p>
                        <ul>
                            <li><a href="infantChild.html">婴孕健康</a></li>
                        </ul>
                    </div>
                    <div class="item">
                        <p>
                            <a href="newsList.html">NEWS</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <!-- 公共top end -->
        <!-- 公共banenr start -->
        <div class="banner-common">
            <img src="img/banner-page-5.jpg">
            <span>News List</span>
        </div>
        <!-- 公共banenr end -->
        <div class="block-box">

            <!-- 新闻详情 start -->
            <div class="news-details">
                <div class="news-details-box ">
                    <div class="info">
                        <div class="date-num block-box">
                            <div class="fl">
                                <span class="max">10</span>
                                2020-06
                            </div>
                            <div class="fr">阅读：<%=details.link%></div>
                        </div>
                        <p class="name"><%=details.title%></p>
                        <div class="share">
                            <img src="img/weibo-logo.png">
                            <img src="img/weixin-logo.png">
                            <a href="/newsDetails?id=<%=id%>&act=link">
                                <img src="img/qq.png">
                            </a>
                        </div>
                        <div class="button block-box">
                            <a class="left">返回列表>></a>
                            <a class="right">返回列表>></a>
                        </div>
                    </div>
                    <div class="detail">
                        <div class="text">
                            <!-- = 是正常输出， - 是转义输出 -->
                            <%-details.content%>
                        </div>
                    </div>
                </div>
            </div>
            <!-- 新闻详情 end -->
            <!-- 公共尾部 start -->
            <div class="footer-common">
                <div class="img">
                    <img class="logo" src="img/logo-min.png" />
                    <span></span>
                    <img src="img/qrcode.jpg" />
                </div>
                <p class="site">kunella Feinlos(库尼拉)中国区官网</p>
                <p>在线邮件：kunella@163.com</p>
                <p>联系地址：陕西省咸阳市万达广场1901-A</p>
            </div>
