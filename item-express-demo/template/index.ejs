<!DOCTYPE html>
<html lang="zh">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>库尼拉</title>
  <link rel="stylesheet" href="css/swiper.min.css" />
  <link rel="stylesheet" href="css/common.css" />
  <link rel="stylesheet" href="css/index.css" />
  <script src="./js/swiper.min.js"></script>
  <script src="./js/jquery-3.5.1.min.js"></script>
</head>

<body>
  <div class="index-box">
    <!-- 公共top start -->
    <div class="common-top">
      <div class="logo fl">
        <img src="img/white-log.png" />
      </div>
      <div class="nav fr">
        <img class="nav-img" src="img/nav-icon.png" />
        <div class="nav-list">
          <div class="title">APPBAR</div>
          <div class="item">
            <p>
              <a href="index.html">首页 </a>
            </p>
          </div>
          <div class="item">
            <p>
              品牌历史
              <img class="right" src="img/right-icon.png" />
            </p>
            <ul>
              <li><a href="brandHistory.html">品牌历史</a></li>
              <li><a href="brandHeritage.html">品牌传承</a></li>
              <li><a href="brandConcept.html">品牌理念</a></li>
              <li><a href="">产地视角</a></li>
            </ul>
          </div>
          <div class="item">
            <p>
              <a href="">产品展示
                <img class="right" src="img/right-icon.png" />
              </a>
            </p>
            <ul>
              <li><a href="proList.html">产品展示</a></li>
            </ul>
          </div>
          <div class="item">
            <p>
              <a href="">婴孕健康
                <img class="right" src="img/right-icon.png" />
              </a>
            </p>
            <ul>
              <li><a href="infantChild.html">婴孕健康</a></li>
            </ul>
          </div>
          <div class="item">
            <p>
              <a href="newsList.html">NEWS</a>
            </p>
          </div>
        </div>
      </div>
    </div>
    <!-- 公共top end -->
    <div class="banner-index swiper-container">
      <div class="swiper-wrapper">
        <!-- 循环结构 -->
        <% for(let i=0;i< banners.length;i++) { %>
        <div class="swiper-slide">
          <div class="img">
            <img src=<%= banners[i].src %> />
          </div>
          <div class="text">
            <p><%= banners[i].subTitle %></p>
          </div>
          <div class="info block-box">
            <div class="img fl">
              <img src="img/logo-min.png" />
            </div>
            <div class="fr span">
              <%= banners[i].title %>
            </div>
          </div>
        </div>
        <% } %>
      </div>
      <!-- Add Pagination -->
      <div class="swiper-pagination"></div>
    </div>
  </div>
</body>
<script src="./js/index.js"></script>

</html>